/* pages/mystudy/mystudy.wxss */
.contentbox{
  margin: 10px;
}
/*签到情况*/
    .BOX{
        width: 100%;
        border: solid 1px #f20686;
        
    }
    .count{
        width: 100%;
        height: 50px;
        background: #f20686;
        color: white;
        text-align: center;
        line-height: 50px;
        font-size: 24px;
        border-bottom: solid 1px #f20686
    }
    .count li{
        list-style: none;
        float: left;
        width: 50%;
    }
    .count li.hidden{
        color: #000;
        background: #fff;
        
    }
    .libox{
        width: 90%;
        margin: auto;
        font-size: 18px;
    }
    .hide{
        display: none;
    }
    .listli{
      margin: 10px;
      height: 30px;
      border-bottom: solid 1px #f20686;
    }
    .ranknum{
      color: #f20686;
    }
    .num1{
      text-align: center;
      width: 20px;
      height: 20px;
      line-height: 20px;
      background: #f20686;
      border-radius: 10px;
      color: white;
    }
    .num2{
      text-align: center;
      width: 20px;
      height: 20px;
      line-height: 20px;
      color: #f20686;
      border: solid 1px #f20686;
      border-radius: 10px;
    }
    .fl{
      float: left;
    }
    .fr{
      float: right;
    }
    .name{
      width: 100px;
      overflow:hidden;
      text-overflow:ellipsis;
      white-space:nowrap;
    }